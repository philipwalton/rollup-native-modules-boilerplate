  },
  "bugs": {
    "url": "https://github.com/philipwalton/rollup-js-modules-boilerplate/issues"
  },
  "keywords": [
    "rollup",
    "module",
    "esm",
    "import",
    "dynamic"
  ],
  "dependencies": {
